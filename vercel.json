{
    "buildCommand": "cd src && npm install && npm start",
    "outputDirectory": "src/dist",
    "builds": [
        {
            "src": "mybackend/mybackend/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/mybackend/mybackend/wsgi.py"
        },
        {
            "source": "/(.*)",
            "destination": "/src/dist/$1"
        }
    ]
}